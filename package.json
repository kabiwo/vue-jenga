{
  "name": "vue-jenga",
  "private": false,
  "version": "0.1.8",
  "description": "A series of tool for vue based on element-plus & pinia & axios.",
  "keywords": [
    "vue",
    "element-plus",
    "axios",
    "pinia"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "build:lib": "vite build --config vite.lib.config.ts",
    "bumpp": "bumpp",
    "deploy": "gh-pages -d dist-doc"
  },
  "exports": {
    "./menu": {
      "import": "./dist/menu.js",
      "types": "./dist/types/menu/index.d.ts"
    },
    "./form": {
      "import": "./dist/form.js",
      "types": "./dist/types/form/index.d.ts"
    },
    "./table": {
      "import": "./dist/table.js",
      "types": "./dist/types/table/index.d.ts"
    },
    "./modal": {
      "import": "./dist/modal.js",
      "types": "./dist/types/modal/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "types": "./dist/types/utils/index.d.ts"
    },
    "./vue-jenga.css": "./dist/vue-jenga.css"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/types",
  "dependencies": {
    "@shikijs/markdown-it": "^3.9.2",
    "@shikijs/twoslash": "^3.9.2",
    "@vueuse/core": "^13.6.0",
    "element-plus": "^2.11.1",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-async": "^2.2.0",
    "pinia": "^3.0.3",
    "postcss-pxtorem": "^6.1.0",
    "radash": "^12.1.1",
    "shiki": "^3.9.2",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.2.2",
    "@iconify/types": "^2.0.0",
    "@iconify/utils": "^2.3.0",
    "@types/node": "^24.2.0",
    "@types/postcss-pxtorem": "^6.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "bumpp": "^10.2.3",
    "gh-pages": "^6.3.0",
    "typescript": "~5.8.3",
    "unocss": "^66.4.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-element-plus": "^0.10.0",
    "unplugin-vue-components": "^28.8.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.0.6",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-purgecss": "^0.2.13",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^2.2.12"
  },
  "peerDependencies": {
    "axios": "^1.11.0",
    "element-plus": "^2.10.5",
    "pinia": "^3.0.3",
    "vue": "^3.5.18"
  }
}